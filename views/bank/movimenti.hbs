<head>

    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">


</head>
<body>
<h1>Ecco i tuoi movimenti bancari</h1>
<div align="left">

<section class="get">
    <h3>Risultati</h3>
    <a href="/get-mov">Aggiorna</a><br><br>
</section>


<div class="container">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>ID movimento</th>
            <th>Data</th>
            <th>Causale</th>
            <th>Valuta</th>
            <th>Importo</th>
        </tr>

        </thead><br>
        {{# each items }}
        <tbody>
        <tr>

            <td>{{ this.movimento_id}}</td>
            <td>{{ this.Data}}</td>
            <td>{{ this.Causale}}</td>
            <td>{{ this.Valuta}}</td>
            <td>{{ this.Importo}}</td>

        </tr>


        </tbody>
        {{/each}}
    </table>
</div>
</div>
</body>


</html>